//- size: (s|m)
mixin room-form__checkin(parameters)
    include ../../buttonfield/buttonfield.pug
    +buttonfield({
        'label': 'ПРИБЫТИЕ',
        'value' : 'ДД.ММ.ГГГГ',
        'icon': 'expand',
        'disabled': true
    })(
        class='room-form__checkin'
        class='room-form__checkin_size_' + parameters.size
    )